          <i class="fa fa-star"></i>
        </div>
      </div> -->
      <% for( var review of deal.reviews ) { %>
      <div class="user-nav__user user-review">
        <% if (review.user.photo) { %>
        <img src=<%=`/img/users/${review.user.photo}`%> alt="User photo" class="user-nav__user-photo review-photo">
        <% } else { %>
        <a href=<%= `/member/${review.user.id}` %> ><img style="margin-top: unset;"
          data-name=<%=review.user.name %> data-color="#85e0ce" class="profile
          user-nav__user-photo"/></a>
          <% } %>
        <div class="review-text">
          <div class="user-nav__user-name"><%= review.user.name %> </div>
          <div class="rev" id="normRev">
            <%= review.review %>
          </div>
          <div style="display: none;" id="editDiv">
          <div class="rev" contenteditable="true" id="revrevrev" style="border: 2px solid black;">
            <%= review.review %>
          </div>
          <button style="right: 0;" id="editRevBtn" onclick="editRevFun(this)" name=<%= `${review._id}/edit` %>>Edit</button>
        </div>
        </div>
        <% if (user) { %>
          <% if (user.id == review.user._id) { %>
         <form action= <%= `/deal/${deal._id}/postedBy/${deal.user}` %> method="GET">
        <select class="three-dots" id="big-three-dots" name="dealOps" onchange="dealOperations(this)">
          <option selected disabled value="empty"></option>
          <option value=<%= `${review._id}/edit` %>>Edit</option>
          <option value=<%= `${review._id}/delete` %>>Delete</option>
          
        </select>
      </form>
        <% } %>
          <% } %>

      </div>
      <% } %>


      <div id="hidden-review">
